{
  "name": "us",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "test": "jasmine",
    "start": "node dist/server.js",
    "watch-node": "nodemon",
    "watch-ts": "tsc -w",
    "watch-browser": "watchify dist/front/main.js -o dist/public/js/main.js -v",
    "watch-sass": "node-sass src/public/css/main.scss dist/public/css/main.css -w src/public/css --include-path node_modules/foundation-sites/scss/",
    "mongo": "cd ~/Developer; mongod --dbpath=mongodb",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node,Sass,Browser\" -c \"yellow.bold,cyan.bold,blue.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\" \"npm run watch-sass\" \"npm run watch-browser\""
  },
  "nodemonConfig": {
    "ignore": [
      "*.json",
      "**/*.json",
      "**/*.map",
      "dist/public/js/main.js"
    ],
    "verbose": false
  },
  "author": "Julien Breiner",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "capitalize": "^1.0.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "express-mongoose-store": "^1.0.7",
    "express-session": "^1.15.6",
    "express-validator": "^5.1.2",
    "foundation-sites": "^6.4.4-rc1",
    "gsap": "^1.20.4",
    "i18n": "^0.8.3",
    "mongoose": "^5.0.13",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pathfinding": "^0.4.18",
    "pixi.js": "^4.7.3",
    "pug": "^2.0.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "socket.io": "^2.1.0",
    "underscore.string": "^3.3.4",
    "watchify": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/capitalize": "^1.0.1",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.11.1",
    "@types/express-session": "^1.15.8",
    "@types/gsap": "^1.19.2",
    "@types/i18n": "^0.8.3",
    "@types/mongoose": "^5.0.9",
    "@types/passport": "^0.4.4",
    "@types/passport-local": "^1.0.33",
    "@types/pathfinding": "0.0.2",
    "@types/pixi.js": "^4.7.2",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/socket.io": "^1.4.33",
    "@types/socket.io-client": "^1.4.32",
    "@types/underscore.string": "0.0.32",
    "node-sass": "^4.8.3",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  }
}